<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
		<ApplicationIcon>SemanticXamlPrint.PDF.ico</ApplicationIcon>
		<Version>3.1.3</Version>
		<AssemblyVersion>3.1.3</AssemblyVersion>
		<FileVersion>3.1.3</FileVersion>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="SemanticXamlPrint.PDF.ico" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="SemanticXamlPrint.PDF.ico" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="PdfSharp.netstandard" Version="1.3.2" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\SemanticXamlPrint\SemanticXamlPrint.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="SemanticXamlPrint.PDF.jpg">
	    <PackagePath>\</PackagePath>
	    <Pack>True</Pack>
	  </None>
	</ItemGroup>

</Project>
